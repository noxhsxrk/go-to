{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "go-to",
  "title": "go to",
  "description": "can go to specific site",
  "icon": "extension-icon.png",
  "author": "noxhsxrk",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "go",
      "title": "Go to Site",
      "description": "Quickly navigate to configured sites",
      "mode": "no-view"
    },
    {
      "name": "manage-sites",
      "title": "Manage Sites",
      "description": "Add, view, or remove site shortcuts",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.1",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "preferences": [
    {
      "name": "siteMappings",
      "type": "textfield",
      "required": false,
      "default": "{}",
      "title": "Default Site Mappings",
      "description": "JSON object mapping site names to URLs"
    }
  ]
}
